<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="rimworld DefsSchema.xsd" xmlns="rimworld">

    <RecipeDef ParentName="SurgeryFlesh">
        <defName>ExtractBioPropertySurgery</defName>
        <label>Extract property</label>
        <workerClass>Zlepper.Rimworld.PersonalitySurgery.Recipe_ExtractBioPropertySurgery</workerClass>
        <workAmount>2500</workAmount>
        <isViolation>true</isViolation>
        <hideBodyPartNames>true</hideBodyPartNames>
        <skillRequirements>
            <Medicine>4</Medicine>
        </skillRequirements>
        <recipeUsers>
            <li>Human</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Brain</li>
        </appliedOnFixedBodyParts>
        <researchPrerequisites>
            <li>PersonalitySurgery</li>
        </researchPrerequisites>
        <surgerySuccessChanceFactor>0.9</surgerySuccessChanceFactor>
    </RecipeDef>
    
