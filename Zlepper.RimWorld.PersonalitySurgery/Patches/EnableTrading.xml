<Patch>

    <Operation Class="PatchOperationAdd">
        <!-- Add items to all traders that can stock genepacks -->
        <xpath>/Defs/TraderKindDef/stockGenerators/li[@Class='StockGenerator_SingleDef' and thingDef='Genepack']/..
        </xpath>
        <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>SurgeryExtractBioTraitItem</thingDef>
                <countRange>0~2</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>SurgeryExtractBioPassionItem</thingDef>
                <countRange>0~2</countRange>
            </li>
        </value>
    </Operation>


</Patch>